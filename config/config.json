{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "admin-form-web-part": {
      "components": [{
        "entrypoint": "./lib/webparts/adminForm/AdminFormWebPart.js",
        "manifest": "./src/webparts/adminForm/AdminFormWebPart.manifest.json"
      }]
    }
  },
  "externals": {
    "jquery": {
      "path": "node_modules/jquery/dist/jquery.min.js",
      "globalName": "jQuery"
    }
  },
  "localizedResources": {
    "AdminFormWebPartStrings": "lib/webparts/adminForm/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js"
  }
}
